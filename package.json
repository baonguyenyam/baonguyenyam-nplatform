{
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:netlify": "NETLIFY=true next build && npm run postbuild:netlify",
    "postbuild:netlify": "./cmd/build-netlify.sh",
    "build:standalone": "./cmd/build-standalone.sh",
    "start:standalone": "./cmd/start-standalone.sh",
    "lint": "next lint --no-cache",
    "format": "npx eslint ./src --fix && npx @biomejs/biome format --write ./src",
    "email": "email dev --port 9000",
    "start": "next start",
    "postinstall": "prisma generate --no-engine",
    "generate": "prisma generate --no-engine && prisma format",
    "reset": "prisma migrate reset --force --skip-seed",
    "migrate": "prisma migrate dev --name init",
    "update": "prisma migrate status --schema=./prisma/schema.prisma",
    "seed": "npx prisma db seed -- --environment development",
    "docker:setup": "./cmd/dev-docker.sh setup",
    "docker:dev": "./cmd/dev-docker.sh dev",
    "docker:start": "./cmd/docker.sh start",
    "docker:stop": "./cmd/docker.sh stop",
    "docker:clean": "./cmd/dev-docker.sh clean",
    "optimize": "./scripts/optimize-performance.sh",
    "optimize:advanced": "./scripts/advanced-optimize.sh",
    "analyze": "cross-env ANALYZE=true next build --turbopack",
    "check-types": "tsc --noEmit",
    "perf:monitor": "node scripts/monitor-performance.js",
    "bundle:analyze": "ANALYZE=true next build --turbopack"
  },
  "prisma": {
    "seed": "tsx src/models/seed/index.ts"
