{
	"functions": {
		"src/app/api/auth/[...nextauth]/route.ts": {
			"maxDuration": 10
		},
		"src/app/api/v1/route.ts": {
			"maxDuration": 10
		}
	},
	"outputDirectory": ".next",
	"buildCommand": "npm run build",
	"regions": [
		"iad1"
	],
	"rewrites": [
		{
			"source": "/api/v1/admin/:path*",
			"destination": "/api/v1/route"
		},
		{
			"source": "/api/v1/public/:path*",
			"destination": "/api/v1/route"
		}
	]
}